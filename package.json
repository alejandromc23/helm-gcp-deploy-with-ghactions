{
  "name": "nodejs-ts-gke-cluster",
  "version": "1.0.0",
  "description": "A simple template for deploying a nodejs typescript project to gke through github actions :)",
  "main": "start.ts",
  "repository": "https://github.com/alejandromc23/nodejs-ts-gke-cluster.git",
  "author": "alejandromc23",
