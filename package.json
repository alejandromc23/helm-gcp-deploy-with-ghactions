    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "express-promise-router": "^4.1.1",
    "http-status": "^1.5.2",
    "typescript": "^4.7.4",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1"
  }
